# Streamlit Secrets Configuration
# Copy this file to secrets.toml and fill in your actual values
# DO NOT commit secrets.toml to version control

[jira]
cloud_id = "your-atlassian-cloud-id-here"
project_key = "your-project-key-here-here"
base_url = "your-atlassian-base-url-here"

# API Authentication
email = "your-email@example.com"
api_token = "your-api-token-here"

# Optional: OAuth Configuration
# [jira.oauth]
# client_id = "your-oauth-client-id"
# client_secret = "your-oauth-client-secret"
